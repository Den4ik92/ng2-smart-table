    "@angular/forms": "^20.3.4",
    "@angular/platform-browser": "^20.3.4",
    "@angular/platform-browser-dynamic": "^20.3.4",
    "@angular/router": "^20.3.4",
    "@nebular/eva-icons": "^16.0.0",
    "@nebular/theme": "^16.0.0",
    "eva-icons": "^1.1.3",
    "nebular-icons": "^1.1.0",
    "ng-packagr": "^20.3.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.1",
    "zone.js": "~0.15.0"
  },
